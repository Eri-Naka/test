drop database if exists sampledb;
create database sampledb;
use database;

create table sampledb(
No int , name varchar(50) , age int , varchar (50));

insert into sampledb value (1,"Yamada",21,"xxx@yahoo.co.jp");
insert into sampledb value(2,"Sato",39,"yyy@google.com");
