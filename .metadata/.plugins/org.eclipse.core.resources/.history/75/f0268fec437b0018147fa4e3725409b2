package com.endomath.action;

public class EndoMathAction {


	private int number;

	private int answer;


	public void setNumber(int number){
		this.number = number;
	}


	public String execute(){
		int a = 1;
		while( a <= 10 ){
			answer = number * a ;

		}
		return "OK";
	}



	public int getAnswer(){
		return answer;
	}

}
